.stellarnav > ul > li > a:before {content:""; display:block; position:absolute; bottom:0; left:0; right:0; height:3px; background:#020D2B; opacity:0; -moz-transition:all 0.3s; -o-transition:all 0.5s; -webkit-transition:all 0.5s; transition:all 0.5s; }
